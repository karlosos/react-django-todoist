{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "firebase": "^6.2.4",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "react": "^16.12.0",
    "react-contenteditable": "^3.3.3",
    "react-dark-mode-toggle": "^0.0.2",
    "react-dom": "^16.12.0",
    "react-icons": "^3.9.0",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard && react-scripts test --watchAll",
    "test-coverage": "standard && react-scripts test --coverage --watchAll",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/firebase.prod.js",
      "!src/hooks/*.js",
      "!src/context/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "^7.2.1",
    "eslint-plugin-react-hooks": "^2.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
